                                 $('#box_cinema_wait').show();
                                 $.post(BASEMETHOD + 'addCinema', {nome:nome, localizacao:localizacao, endereco:endereco, fone:fone},
                                 function(data) {
                                   $('#box_cinema_wait').hide();
