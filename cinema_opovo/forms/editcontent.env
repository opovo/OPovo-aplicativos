            });
            $(this).find('textarea').map(function(){
                this.name = this.name.replace(/([a-z]*)[0-9]*/, '$1' + (nft+1));
                this.id = this.id.replace(/([a-z]*)[0-9]*/, '$1' + (nft+1));
